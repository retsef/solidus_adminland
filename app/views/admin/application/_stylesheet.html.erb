<%#
# Stylesheet Partial

This partial imports the necessary stylesheets on each page.
By default, it includes the application CSS,
but each page can define additional CSS sources
by providing a `content_for(:stylesheet)` block.
%>

<!-- Tabler -->
<%= stylesheet_link_tag "https://unpkg.com/@tabler/core@latest/dist/css/tabler.min.css", media: 'all', 'data-turbo-track': 'reload' %>
<!-- Tabler Vendors -->
<%= stylesheet_link_tag "https://unpkg.com/@tabler/core@latest/dist/css/tabler-vendors.min.css", media: 'all', 'data-turbo-track': 'reload' %>
<!-- Tabler Icons -->
<%= stylesheet_link_tag "https://unpkg.com/@tabler/icons@latest/iconfont/tabler-icons.min.css", media: 'all', 'data-turbo-track': 'reload' %>

<%= stylesheet_link_tag 'admin', media: 'all', 'data-turbo-track': 'reload' %>

<%# Administrate::Engine.stylesheets.each do |css_path|
  <%=# stylesheet_link_tag css_path
#%>

<%= yield :stylesheet %>
